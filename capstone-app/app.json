{
  "expo": {
    "name": "capstone-app",
    "slug": "capstone-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/imgs/iconLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/imgs/react-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/imgs/react-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/imgs/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
